<Application x:Class="DereTore.Applications.StarlightDirector.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="UI/Windows/MainWindow.xaml"
             ShutdownMode="OnMainWindowClose"
             Startup="App_OnStartup" Exit="App_OnExit" DispatcherUnhandledException="App_OnDispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Fluent;Component/Themes/Office2010/Generic.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/StarlightDirector;component/Resources/Directories/Images.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/StarlightDirector;component/Resources/Directories/Icons.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/StarlightDirector;component/Resources/Directories/Strings.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/StarlightDirector;component/Resources/Directories/Painting.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/StarlightDirector;component/Resources/Directories/Converters.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
