<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="ProjectFileFilter">Starlight Director Project (*.sldproj)|*.sldproj</system:String>
    <system:String x:Key="CsvFileFilter">Comma Separated Values (*.csv)|*.csv</system:String>
    <system:String x:Key="WaveFileFilter">Wave Audio (*.wav)|*.wav</system:String>
    <system:String x:Key="AcbFileFilter">CRI ACB Archive (*.acb)|*.acb</system:String>
    <system:String x:Key="BdbFileFilter">Score Database (*.bdb)|*.bdb</system:String>
    <system:String x:Key="ExitPrompt">The project is changed. Do you want to save it first?</system:String>
    <system:String x:Key="PreviewNotImplemented">The Preview function is not implemented yet. You can export the score to a CSV file and use ScoreEditor to view it.</system:String>
    <system:String x:Key="ConfirmDeleteBar">You are going to delete a measure. This operation cannot be reverted. Proceed?</system:String>
